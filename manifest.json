{
    "manifest_version": 2,
    "name": "HID control prevention",
    "version": "0.1",

    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "content-scripts.js"
            ],
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_start"
        }
    ]
}
